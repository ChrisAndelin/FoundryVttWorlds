er class=\"card-footer\">\n        <span>Martial Melee</span>\n        <span>Equipped</span>\n        <span>Proficient</span>\n        <span>1 Action</span>\n        <span>5 Feet</span>\n    </footer>\n</div>","speaker":{"scene":"kx63pEWTsKVWAsKV","token":"JewYykrAJXAYNFT6","actor":"qilPckbaptu7DhCu","alias":"Darthie Hark"},"whisper":[],"blind":false,"emote":false,"flags":{"core":{"canPopout":true}},"_id":"7uxst1JQecD7qQ3K"}
