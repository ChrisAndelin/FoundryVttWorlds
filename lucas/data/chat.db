v>\n\n    <footer class=\"card-footer\">\n        <span>Cantrip</span>\n        <span>V</span>\n        <span>1 Action</span>\n        <span>1 Creature</span>\n        <span>60 Feet</span>\n        <span>Instantaneous</span>\n    </footer>\n</div>","speaker":{"scene":"4lotUZEdLPs9ZSaR","token":"L8ytWSk3LO9vDWRw","actor":"pno06PihHvCJgxIS","alias":"Willow L'Faux"},"whisper":[],"blind":false,"emote":false,"flags":{"core":{"canPopout":true}},"_id":"gbI0AXqnKWrcnV4d"}
