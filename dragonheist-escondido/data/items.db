{"_id":"YzMxZmRhNGJiYjlj","name":"Special Equipment. (Kaevia Cynavern)","permission":{"default":0},"type":"feat","data":{"description":{"value":"<p>Kaevja carries a yellow elemental gem. In battle, Kaevja breaks the elemental gem, summoning an earth elemental and commanding it to attack her enemies.</p>","chat":"","unidentified":""},"source":"","activation":{"type":"","cost":0,"condition":""},"duration":{"value":0,"units":""},"target":{"value":"","units":"","type":""},"range":{"value":"","long":"","units":""},"uses":{"value":0,"max":0,"per":""},"consume":{"type":"","target":null,"amount":null},"ability":"","actionType":"","attackBonus":0,"chatFlavor":"","critical":null,"damage":{"parts":[],"versatile":""},"formula":"","save":{"ability":"","dc":null,"scaling":"spell"},"requirements":"","recharge":{"value":0,"charged":false}},"folder":"ZTAxOTk4YTI1ZTYx","sort":15870000,"flags":{},"img":"worlds/dragonheist-escondido/characters/169_-_Kaevia_Cynavern/avatar.png"}
