ice":0,"attuned":false,"equipped":true,"rarity":"","identified":true},"sort":170000,"flags":{},"img":"worlds/phandelver-template/characters/030_-_Human_Fighter__28Folk_Hero_29/token.png","effects":[]},{"_id":"YmU5OWY5ZWIyYmVl","name":"Carpenter's Tools","type":"tool","data":{"description":{"value":"<p>These special tools include the items needed to pursue a craft or trade. Proficiency with a set of artisan's tools lets you add your proficiency bonus to any ability checks you make using the tools in your craft. Each type of artisan's tools requires a separate proficiency.</p><p></p><p></p>","chat":"","unidentified":""},"source":"","quantity":1,"weight":1,"price":0,"attuned":false,"equipped":true,"rarity":"","identified":true,"ability":"str","chatFlavor":"","proficient":0,"damage":{"parts":[]}},"sort":180000,"flags":{},"img":"systems/dnd5e/icons/items/weapons/axe-wood.jpg","effects":[]},{"_id":"ZWI4MTQ1NDZiMmYy","name":"Shovel","type":"loot","data":{"description":{"value":"<p></p>","chat":"","unidentified":""},"source":"","quantity":1,"weight":1,"price":0,"attuned":false,"equipped":true,"rarity":"","identified":true,"damage":{"parts":[]}},"sort":190000,"flags":{},"img":"systems/dnd5e/icons/items/inventory/pickaxe.jpg","effects":[]},{"_id":"NTU1MTQ2OWU0NDI3","name":"Iron Pot","type":"backpack","data":{"description":{"value":"<p></p>","chat":"","unidentified":""},"source":"","quantity":1,"weight":1,"price":0,"attuned":false,"equipped":true,"rarity":"","identified":true,"capacity":{"type":"weight","value":8.4,"weightless":false},"currency":{"cp":0,"sp":0,"ep":0,"gp":0,"pp":0},"damage":{"parts":[]}},"sort":200000,"flags":{},"img":"systems/dnd5e/icons/items/inventory/cauldron.jpg","effects":[]},{"_id":"ZWYxOTk0ZTllODk2","name":"Commoner's Clothes","type":"loot","data":{"description":{"value":"<p></p>","chat":"","unidentified":""},"source":"","quantity":1,"weight":1,"price":0,"attuned":false,"equipped":true,"rarity":"","identified":true},"sort":210000,"flags":{},"img":"worl