{"_id":"9T3H3Nbx2NlyaSZF","name":"Update Represented Actor","permission":{"default":0,"ZjdmMWEyMmE2NzVk":3},"type":"script","sort":100001,"flags":{},"scope":"global","command":"(async ()=>{\r\n   console.log(\"starting\");\r\n   for(let token of canvas.tokens.placeables)\r\n   {\r\n      if (!token.data)\r\n      {\r\n         console.log(\"error: no token.data\");\r\n         continue;\r\n      }\r\n      if (!token.data.name)\r\n      {\r\n         console.log(\"error: no token.data.name\");\r\n         continue;\r\n      }\r\n      console.log(\"token = \" + token.data.name);\r\n      let actorName = game.actors.getName(token.data.name);\r\n      if (!actorName)\r\n      {\r\n         console.log(\"could not find actor name for token \" + token.data.name);\r\n      }\r\n      else\r\n      {\r\n         //if (!token.data.actorId)\r\n         //{\r\n            console.log(\"setting \" + token.data.name + \" = \" + actorName.id);\r\n            await token.update({ actorId : actorName.id });\r\n         //}\r\n         //else\r\n         //{\r\n          //  console.log(\"actorId already exists for \" + token.data.name + \" was \" + token.data.actorId);\r\n         //}\r\n      }\r\n   }\r\n   console.log(\"ending\");\r\n})();","author":"ZjdmMWEyMmE2NzVk","img":"icons/svg/dice-target.svg","actorIds":[]}
