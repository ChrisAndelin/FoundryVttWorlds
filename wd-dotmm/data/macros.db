{"_id":"Z1PVcJrfjEqwC5zZ","name":"Remove Top Bar","type":"script","author":"ZjdmMWEyMmE2NzVk","img":"icons/svg/dice-target.svg","scope":"global","command":"(async ()=>{\n   console.log(\"starting\");\n   for(let token of canvas.tokens.placeables)\n   {\n      if (!token.data)\n      {\n         console.log(\"error: no token.data\");\n         continue;\n      }\n      if (!token.data.name)\n      {\n         console.log(\"error: no token.data.name\");\n         continue;\n      }\n      console.log(\"token = \" + token.data.name);\n      await token.data.update({ 'bar2.attribute' : 'None' });\n      await token.data.update({ 'bar1.attribute' : 'attributes.hp' });\n   }\n   console.log(\"ending\");\n})();","folder":null,"sort":100001,"permission":{"default":0,"ZjdmMWEyMmE2NzVk":3},"flags":{"combat-utility-belt":{"macroTrigger":""}}}
