{"_id":"YWU2MzRhNGZlMWFi","name":"Shield (Apprentice Wizard)","permission":{"default":0},"type":"spell","data":{"description":{"value":"<p>An invisible barrier of magical force appears and protects you. Until the start of your next turn, you have a +5 bonus to AC, including against the triggering attack, and you take no damage from magic missile.</p>","chat":"","unidentified":""},"source":"","activation":{"type":"reaction","cost":1,"condition":"which you take when you are hit by an attack or targeted by the magic missile spell"},"duration":{"value":1,"units":"round"},"target":{"value":"","units":"self","type":"self"},"range":{"value":"","long":"","units":"self"},"uses":{"value":0,"max":0,"per":""},"consume":{"type":"","target":null,"amount":null},"ability":"","actionType":"util","attackBonus":0,"chatFlavor":"","critical":null,"damage":{"parts":[],"versatile":""},"formula":"","save":{"ability":"","dc":null,"scaling":"spell"},"level":1,"school":"abj","components":{"value":"","vocal":true,"somatic":true,"material":false,"ritual":false,"concentration":false},"materials":{"value":"","consumed":false,"cost":0,"supply":0},"preparation":{"mode":"prepared","prepared":true},"scaling":{"mode":"none","formula":""}},"folder":"ZjhmY2MzYzY0MGE3","sort":3110000,"flags":{"_sheetTab":"description"},"img":"systems/dnd5e/icons/spells/protect-magenta-1.jpg"}
