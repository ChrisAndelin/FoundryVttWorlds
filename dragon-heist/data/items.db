{"_id":"MTI5NzA0NmEzNjFm","name":"Fire Bolt (Losser Mirklav)","permission":{"default":0},"type":"spell","data":{"description":{"value":"<p>You hurl a mote of fire at a creature or object within range. Make a ranged spell attack against the target. On a hit, the target takes 1d10 fire damage. A flammable object hit by this spell ignites if it isn’t being worn or carried.</p>","chat":"","unidentified":""},"source":"","activation":{"type":"action","cost":1,"condition":""},"duration":{"value":0,"units":"inst"},"target":{"value":"","units":"","type":"creature"},"range":{"value":120,"long":"","units":"ft"},"uses":{"value":0,"max":0,"per":""},"consume":{"type":"","target":null,"amount":null},"ability":"","actionType":"rsak","attackBonus":0,"chatFlavor":"","critical":null,"damage":{"parts":[["1d10","fire"]],"versatile":""},"formula":"","save":{"ability":"","dc":null,"scaling":"spell"},"level":0,"school":"evo","components":{"value":"","vocal":true,"somatic":true,"material":false,"ritual":false,"concentration":false},"materials":{"value":"","consumed":false,"cost":0,"supply":0},"preparation":{"mode":"prepared","prepared":true},"scaling":{"mode":"none","formula":""}},"folder":"ZjhmY2MzYzY0MGE3","sort":15180000,"flags":{},"img":"systems/dnd5e/icons/spells/fireball-red-1.jpg"}
